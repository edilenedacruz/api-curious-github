<div class="dashboard" style="width:800px; margin: 0 auto;">
  <h1>Starred Repos</h1>
  <br>
    <% @stars.map do |repo| %>
      <p><a href="https://www.github.com/<%= repo.full_name %>"><%= repo.full_name %></a></p>
      <p><%= repo.description %></p>
      <p>Forks <%= repo.forks_count %> -  Updated at <%= time_ago_in_words(repo.updated_at) %></p>
      <hr>
    <% end %>
    <center><%= link_to "Return to Dashboard", user_dashboard_path(current_user), class: "button" %></center>
</div>
